{
  "name": "chatgpt-memory-toolkit",
  "version": "1.0.0",
  "description": "A powerful Chrome extension for exporting ChatGPT memories with one click",
  "main": "src/scripts/background.js",
  "scripts": {
    "build": "node build/build.js",
    "dev": "node build/dev.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.{js,html,css}",
    "package": "npm run build && node build/package.js"
  },
  "keywords": [
    "chatgpt",
    "memory",
    "export",
    "chrome-extension",
    "ai",
    "automation",
    "productivity"
  ],
  "author": {
    "name": "ChatGPT Memory Toolkit Team",
    "email": "contact@chatgpt-memory-toolkit.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chatgpt-memory-toolkit/extension.git"
  },
  "bugs": {
    "url": "https://github.com/chatgpt-memory-toolkit/extension/issues"
  },
  "homepage": "https://github.com/chatgpt-memory-toolkit/extension#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
